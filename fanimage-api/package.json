  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "rm -rf ./dist && tsc-watch --onSuccess \"node ./dist/server.js\"",
    "refresh": "rm -rf ./node_modules ./package-lock.json ./dist && npm install && npm run dev",
    "restart": "dropdb fanimageImageRepository && createdb fanimageImageRepository",
    "test": "jest"
  },
  "author": "Gerry Saporito",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
